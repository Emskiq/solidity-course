pragma solidity ^0.8.13;

import {Script, console} from "forge-std/Script.sol";
import {BasicNFT} from "src/BasicNFT.sol";

import {DevOpsTools} from "lib/foundry-devops/src/DevOpsTools.sol";


contract MintBasicNft is Script {
    string public constant PUG_URI = "ipfs://bafybeig37ioir76s7mg5oobetncojcm3c3hxasyd4rvid4jqhy4gkaheg4/?filename=0-PUG.json";

    function run() public {
        // does not work on sepolia test net :'(
        // address basicNFTContractAddress = DevOpsTools.get_most_recent_deployment("BasicNFT", block.chainid);

        // --------------------------------------------------------
        // hardcoded for the latest deployed ERC721 (BasicNFT) contract deployed
        address basicNFTContractAddress = 0x72fFAA3C17AA6607C903DE726d9Bd57958091Ea4;
        mintNftOnContractAddress(basicNFTContractAddress, PUG_URI);
    }

    function mintNftOnContractAddress(address contractAddr, string memory uri) public {
        vm.startBroadcast();
        BasicNFT(contractAddr).mintNFT(uri);
        vm.stopBroadcast();
    }
}


